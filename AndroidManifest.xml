<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.homechef">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,ExtraText">


        <activity android:name="com.homechef.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.homechef.UserLogin"/>
        <activity android:name="com.homechef.ChefLogin"/>
        <activity android:name="com.homechef.UserRegister"/>
        <activity android:name="com.homechef.ChefRegister"/>
        <activity android:name="com.homechef.Chefs"/>
        <activity android:name="com.homechef.ChefList"/>
        <activity android:name="com.homechef.ChefPage"/>
        <activity android:name="com.homechef.Order"/>
        <activity android:name="com.homechef.Payment"/>
        <activity android:name="com.homechef.Location"/>
        <activity android:name="com.homechef.User"/>
        <activity android:name="com.homechef.Item"/>
        <activity android:name="com.homechef.Chef"/>

    </application>

</manifest>